@font-face {
  font-family: 'iconfont';
  src: url('data:font/truetype;charset=utf-8;base64,')
    format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.i-weather-font {
  font-family: 'iconfont' !important;
}

.view-date,
.skycon-view,
.view-humidity {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 750rpx;
}
.view-date-item {
  display: block;
  width: 50rpx;
  text-align: center;
  font-size: 20rpx;
}
.view-skycon-item {
  width: 50rpx;
  text-align: center;
  margin-top: 20rpx;
  padding-top: 10rpx;
  border-top: 1px solid #ddd;

}
.view-skycon-item-icon {
  display: block;
  font-size: 40rpx;
}
.view-skycon-item-text {
  font-size: 15rpx;
  display: block;
}
.view-humidity-item{
border: 1px solid #0050b3;
border-radius: 50%;
font-size: 18rpx;
width: 35rpx;
height: 35rpx;
line-height: 35rpx;
text-align: center;
background-color: #bae7ff;
margin-top:20rpx;
}
ec-canvas {
  width: 750rpx;
  height: 560rpx;
}

.local {
  padding: 40rpx;
  text-align: center;
  font-size: 30rpx;
  line-height: 1.2;
}

.general {
  display: flex;
  justify-content: space-between;
  margin: 0 40rpx;
  align-items: center;
}

.tip-icon {
  margin-right: 10rpx;
  vertical-align: bottom;
  font-size: 40rpx;
}

.tip-main-icon {
  vertical-align: bottom;
  font-size: 150rpx;
}
.week-forecast {
  white-space: nowrap;
  display: flex;
  margin-top: 40rpx;
}
.week-forecast .item {
  display: inline-block;
  border-left: 1rpx solid gray;
  padding: 0 10px;
  text-align: center;
}
.week-forecast .item:nth-of-type(1) {
  border-left: none;
}

.today-temperature {
  text-align: center;
  font-size: 70rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  /* width: 200rpx; */
  
}

.today-forecast {
  text-align: center;
  font-size: 20rpx;
  color: #333;
  margin-bottom: 30rpx;
}
.today-humidity{
    transform: scale(0.3);
    transform-origin: 0 75%;
    margin-left: 15rpx;
    color:#69c0ff;
    width: 0;
}
.template-view {
  margin-top: 20rpx;
  box-sizing: border-box;
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  position: relative;
  width: 750rpx;
  height: 460rpx;
}

